{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/users",
        "dest": "./src/routes/user.routes.js",
        "methods": ["GET"]
      },
      {
        "src": "/users",
        "dest": "/index.js",
        "methods": ["POST"]
      },
      {
        "src": "/users",
        "dest": "/index.js",
        "methods": ["PUT"]
      },
      {
        "src": "/users",
        "dest": "/index.js",
        "methods": ["DELETE"]
      }
    ]
  }